import sys

ans = 0
ant=0

ant = [1,2,3,4,5]
for line in sys.stdin:
    try:
        ans += int(line)
        ant *= int(line) 
    except:
        ans += float(line)
        ant *= float(line)

print(ans)
print(ant[0::2])

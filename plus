import sys

s = 0
t = 1

a=3
b=6
a < b
c=a-1
def f(a,b):
    return (a*a+b*b)**0.5

for y in range(a,b+1):
    try:
        c=c+1
        s=s+float(y)
        t=t*float(y)
        
    except:
        c=c+1
        s += int(y)
        t *= int(y)


if a+1 == b:
    print(a,c,s,t,f(a,b))
    print(c,s-a,t/a)
elif a+2 == b:
    print(a,c-1,c,s,t,f(a,b))
    print(c-1,c,s-a,t/a)
elif a+2 < b:
    print(a,a+1,a+2,c,s,t,f(a,b))
    print(c-2,c-1,c,s-a,t/a)
  



